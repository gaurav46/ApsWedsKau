function ValidateForm()
  {
     if (!(frm.Keyboard.checked||frm.AcousticGuitar.checked||frm.ClassicalGuitar.checked||frm.FlamencoGuitar.checked||frm.StringGuitar.checked||frm.ElectricGuitar.checked||frm.BassGuitar.checked||frm.MusicTheory.checked||frm.CarnaticVocal.checked||frm.Tabla.checked||frm.Mridangam.checked||frm.IndianViolin.checked))
       {
       alert('Please select the course.');
       return false;
       }
       NameTxt= document.frm.Name.value;
       NameTxtreplace=NameTxt.replace(/ /g, "");
       var namevalid= /^([a-zA-Z ]+)$/;
       if(NameTxt=="")
       {
	alert("Please Enter the Name"); 
	document.frm.Name.focus();
	return false;
        }	
        if(NameTxtreplace.search(namevalid)==-1)
	{
	alert("Please enter a valid name. Name should not contain numbers or special characters.");
        document.frm.Name.value=""; 
	document.frm.Name.focus();
	return false;
	}
    NRIC=document.frm.NRIC_BC.value;
	NRICNo= NRIC.replace(/ /g,"");
        var validNRIC= /^([a-zA-Z0-9]+)$/; 
        if(NRIC=="")
        {
	alert("Please Enter the NRIC"); 
	document.frm.NRIC_BC.focus();
	return false;
        }
        if(NRICNo.search(validNRIC)==-1 || NRICNo.length<9 ||!(isNaN(NRICNo.charAt(0))) ||!(isNaN(NRICNo.charAt(8)))||isNaN(NRICNo.substring(2,8)))
       {
	alert("Please Enter the valid NRIC");
      document.frm.NRIC_BC.value=""; 
	document.frm.NRIC_BC.focus();
	return false;
        } 
        if (frm.DOB.value.replace(/ /g, "")=="")
        {
	alert("Please select the Date Of Birth")
	frm.DOB.focus()
	return false
	}

       ParentNameTxt= document.frm.ParentSpouseGuardianName.value;
       ParentNameTxtreplace=ParentNameTxt.replace(/ /g, "");
       if(ParentNameTxt!="")
       {
         if(ParentNameTxtreplace.search(namevalid)==-1)
         {
         alert("Please enter a valid Name of Parent / Spouse / Guardian.Name of Parent / Spouse / Guardian should not contain numbers or special characters.");
         document.frm.ParentSpouseGuardianName.value=""; 
         document.frm.ParentSpouseGuardianName.focus();
         return false;
         }
       }

        if (frm.Street.value.replace(/ /g, "")=="")
        {
	alert("Please enter the Address")
	frm.Street.focus()
	return false
	}
        if (frm.ZIP.value.replace(/ /g, "")==""){
	alert("Please enter the Postal Code")
	frm.ZIP.focus()
	return false
	} 	
        Zipcode=document.frm.ZIP.value;
	Zipc= Zipcode.replace(/,/g,"");
        if (isNaN(Zipc))
	{
	alert("Please Enter the Valid Postal Code");
	document.frm.ZIP.value="";
	document.frm.ZIP.focus();
	return false;
	}
        i= document.frm.Email.value;
       	var re=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
	if(i=="")
	{
	alert("Please Enter the Mail ID"); 
	document.frm.Email.focus();
	return false;
	}
	if(i.search(re)==-1)
	{
	alert("Please Enter valid Mail ID");
        document.frm.Email.value=""; 
	document.frm.Email.focus();
	return false;
	}
        if(document.frm.HomePhone.value=="" && document.frm.WorkPhone.value=="")
        {
        alert("Please enter the Home number or Office number");
        document.frm.HomePhone.focus();
	return false;
        } 
	HP=document.frm.HomePhone.value;
	HomeP= HP.replace(/,/g,"");
	if (isNaN(HomeP))
	{
	alert("Please Enter valid Phone no");
	document.frm.HomePhone.value="";
	document.frm.HomePhone.focus();
	return false;
	}
	WP=document.frm.WorkPhone.value;
	WorkP= WP.replace(/,/g,"");
	if (isNaN(WorkP))
	{
	alert("Please Enter valid Phone no");
	document.frm.WorkPhone.value="";
	document.frm.WorkPhone.focus();
	return false;
	}
	MP=document.frm.Mobile.value;
	MobileP= MP.replace(/,/g,"");
	if (isNaN(MobileP))
	{
	alert("Please Enter valid Mobile no");
	document.frm.Mobile.value="";
	document.frm.Mobile.focus();
	return false;
	}
	PP=document.frm.Pager.value;
	PagerP= PP.replace(/,/g,"");
	if (isNaN(PagerP))
	{
	alert("Please Enter valid Pager no");
	document.frm.Pager.value="";
	document.frm.Pager.focus();
	return false;
	}
     
      
	
  }
</script>

